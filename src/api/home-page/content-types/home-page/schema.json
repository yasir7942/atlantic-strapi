      "required": true,
      "targetField": "title"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-infomation"
    },
    "banner": {
      "type": "component",
      "repeatable": false,
      "component": "layout.top-banner"
    },
    "bodyHeading1": {
      "type": "string"
    },
    "bodyText1": {
      "type": "text"
    },
    "bodyHeading2": {
      "type": "string"
    },
    "bodyText2": {
      "type": "text"
    },
    "imageText": {
      "type": "dynamiczone",
      "components": [
        "layout.image",
        "layout.text-image"
      ]
    },
    "infoBlock": {
      "type": "component",
      "repeatable": false,
      "component": "layout.text-image"
    },
    "bodyImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
