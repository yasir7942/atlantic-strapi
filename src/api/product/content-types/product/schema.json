{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "packing": {
      "type": "text"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-infomation"
    },
    "grade": {
      "type": "string",
      "required": true
    },
    "api": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "acea": {
      "type": "string"
    },
    "productImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "name": {
      "type": "string",
      "placeholder": "Short Product Name",
      "description": "Like: Atlantic syntectic 10w-40"
    },
    "related_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "related_products"
    },
    "tdsTitle1": {
      "type": "string",
      "required": false
    },
    "tdsTitle2": {
      "type": "string",
      "required": false
    },
    "tdsTitle3": {
      "type": "string",
      "required": false
    },
    "tdsDescription": {
      "type": "text",
      "required": false
    },
    "application": {
      "type": "component",
      "repeatable": true,
      "component": "layout.tsdlist"
    },
    "recommendations": {
      "type": "component",
      "repeatable": true,
      "component": "layout.tsdlist"
    },
    "tdstable": {
      "type": "component",
      "repeatable": true,
      "component": "layout.tdstable"
    },
    "benifits": {
      "type": "component",
      "repeatable": true,
      "component": "layout.tdstable"
    },
    "tdsPacking": {
      "type": "text"
    },
    "productSchema": {
      "type": "component",
      "repeatable": false,
      "component": "seo.product-schema"
    },
    "msds_register": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::msds-register.msds-register",
      "inversedBy": "product"
    },
    "TDSFile": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "MSDSFile": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
