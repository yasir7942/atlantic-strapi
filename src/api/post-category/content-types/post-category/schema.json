{
  "kind": "collectionType",
  "collectionName": "post_categories",
  "info": {
    "singularName": "post-category",
    "pluralName": "post-categories",
    "displayName": "PostCategory"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "post_categories"
    }
  }
}
